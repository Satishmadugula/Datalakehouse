{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": "*",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::raw/*",
        "arn:aws:s3:::bronze/*",
        "arn:aws:s3:::silver/*",
        "arn:aws:s3:::gold/*",
        "arn:aws:s3:::iceberg-metadata/*",
        "arn:aws:s3:::raw",
        "arn:aws:s3:::bronze",
        "arn:aws:s3:::silver",
        "arn:aws:s3:::gold",
        "arn:aws:s3:::iceberg-metadata"
      ]
    }
  ]
}
